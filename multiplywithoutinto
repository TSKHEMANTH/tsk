#include <stdio.h>

int main()
{
    int a,b,i,ab;
    printf("enter the numbers: \n");
    scanf("%d%d",&a,&b);
    i=1;
    ab=0;
    while (i<=b)
    {
        ab=ab+a;
        i++;
    }
    printf("%d multiplied by %d is %d",a,b,ab);
    return 0;
}
